{
  "name": "skript",
  "description": "All essential tools for developing skript (.sk) code",
  "displayName": "Skript",
  "version": "0.2.0",
  "author": "John Heikens",
  "publisher": "JohnHeikens",
  "license": "SEE LICENSE IN LICENSE",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.73.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnHeikens/SkriptExtensionPack"
  },
  "categories": [
    "Extension Packs",
    "Language Packs"
  ],
  "extensionPack": [
    "johnheikens.intelliskript",
    "TabNine.tabnine-vscode",
    "wayou.vscode-todo-highlight"
  ],
  "keywords": [
    "skript",
    "sk",
    "intellisense",
    "debug"
  ],
  "contributes": {
    "configuration": {
      "todohighlight.include": {
        "type": "string",
        "default": "**/*.sk,**/*.js,**/*.jsx,**/*.ts,**/*.tsx,**/*.html,**/*.php,**/*.css,**/*.scss",
        "description": "makes TODO highlight work with skript files"
      }
    }
  }
}
